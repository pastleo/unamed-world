{
  "name": "unnamed-world",
  "version": "0.0.1",
  "description": "exploration of imagination",
  "main": "index.js",
  "scripts": {
    "dev": "esbuild src/web.ts src/web-worker.ts --bundle --sourcemap --outdir=www --servedir=www",
    "tc": "tsc -w -noEmit",
    "bundle": "rm -rf dist && cp -r www dist && esbuild src/web.ts src/web-worker.ts --bundle --minify --outdir=dist --entry-names=[dir]/[name]-[hash] --metafile=dist/meta.json && esbuild-meta-into-html-script-src dist/meta.json www/index.html dist/index.html src/ dist/"
  },
  "author": "PastLeo",
  "license": "ISC",
  "dependencies": {
    "@types/three": "^0.131.0",
    "comlink": "^4.3.1",
    "esbuild-meta-into-html-script-src": "^1.0.3",
    "isomorphic-webcrypto": "^2.3.8",
    "three": "^0.131.3",
    "unnamed-network": "file:../unnamed-network",
    "ws": "^8.2.0"
  },
  "devDependencies": {
    "esbuild": "^0.12.20",
    "typescript": "^4.3.5"
  }
}
