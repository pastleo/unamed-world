{
  "name": "unamed-world",
  "version": "0.0.1",
  "description": "world for exploration of imagination",
  "main": "index.js",
  "scripts": {
    "dev": "esbuild src/web.ts src/web-worker.ts --bundle --sourcemap --outdir=www --servedir=www",
    "tc": "tsc -w -noEmit",
    "ci-tc": "tsc -noEmit",
    "build": "rm -rf dist && cp -r www dist && esbuild src/web.ts src/web-worker.ts --bundle --minify --outdir=dist --entry-names=[dir]/[name]-[hash] --metafile=dist/meta.json && esbuild-meta-into-html-script-src dist/meta.json www/index.html dist/index.html src/ dist/"
  },
  "author": "PastLeo",
  "license": "ISC",
  "dependencies": {
    "@types/three": "^0.131.0",
    "comlink": "^4.3.1",
    "delatin": "github:mapbox/delatin#7edaefd739bab8b653296fbc026dbd43e525f09f",
    "esbuild-meta-into-html-script-src": "^1.0.3",
    "ipfs-core": "^0.11.1",
    "ipfs-core-min": "^0.0.2",
    "ipfs-pubsub-room": "^2.0.1",
    "isomorphic-webcrypto": "https://github.com/pastleo/isomorphic-webcrypto/releases/download/2.3.8-without-rn/isomorphic-webcrypto-without-rn.tar.gz",
    "libp2p-websockets": "^0.16.2",
    "localforage": "^1.10.0",
    "multiformats": "^9.4.10",
    "superstruct": "^0.15.3",
    "three": "^0.131.3",
    "uint8arrays": "^3.0.0",
    "utility-types": "^3.10.0",
    "ws": "^8.2.0"
  },
  "devDependencies": {
    "esbuild": "^0.12.20",
    "typescript": "^4.3.5"
  }
}
